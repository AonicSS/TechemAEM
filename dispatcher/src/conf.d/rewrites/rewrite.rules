# Exceptions - avoid these automated tests URLs being rewritten (stops further rewrites)
RewriteRule ^/content/test-site/(.*)\.html$ - [NC,L]

# rewrite for root redirect
RewriteRule ^/content/techem/(.*).html?$ /$1 [R=301,L]

# Redirect the .html URL to extensionless URL
RewriteCond %{REQUEST_URI} \.html$
RewriteRule ^/(.*).html$ /$1 [R=301,L,QSA]

# Redirect the URL with trailing slash to the preferred one without /
RewriteRule ^/?(.+)/$ /$1 [R=301,L]

# rewrite for sitemap
RewriteRule ^/(.*).sitemap.xml$ /content/techem/$1.sitemap.xml [PT,L]

# Append the .html for extensionless URLs before sending to publisher
# Do not rewrite requests under /etc.clientlibs (js,css,fonts,images,etc,.)
RewriteCond %{REQUEST_URI} !^/apps
RewriteCond %{REQUEST_URI} !^/bin
RewriteCond %{REQUEST_URI} !^/content
RewriteCond %{REQUEST_URI} !^/etc
RewriteCond %{REQUEST_URI} !^/home
RewriteCond %{REQUEST_URI} !^/libs
RewriteCond %{REQUEST_URI} !^/saml_login
RewriteCond %{REQUEST_URI} !^/system
RewriteCond %{REQUEST_URI} !^/tmp
RewriteCond %{REQUEST_URI} !^/var
RewriteRule ^/(.*)$ /content/techem/$1.html [PT,L,QSA]

