<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        cq:template="/conf/techem/settings/wcm/templates/home-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="techem/components/structure/home-page">
        <experiencefragment
                jcr:primaryType="nt:unstructured"
                sling:resourceType="techem/components/experiencefragment"
                editable="{Boolean}true"/>
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="techem/components/container">
            <home-stage
                jcr:primaryType="nt:unstructured"
                sling:resourceType="techem/components/home-stage/"
                editable="{Boolean}true"/>
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="techem/components/container"
                editable="{Boolean}true"
                layout="responsiveGrid"/>
        </root>
        <experiencefragment_2088209270
                jcr:primaryType="nt:unstructured"
                sling:resourceType="techem/components/experiencefragment"
                editable="{Boolean}true"/>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}768"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
