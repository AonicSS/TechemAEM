<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:template="/conf/techem/settings/wcm/templates/landing-page"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="techem/components/structure/home-page">
        <root
                jcr:primaryType="nt:unstructured"
                sling:resourceType="techem/components/container">
            <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="techem/components/container"
                    layout="responsiveGrid"/>
            <home-stage
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="techem/components/home-stage"
                    autopauseDisabled="false"
                    autoplay="false"
                    transitionSpeed="10000">
                <home_stage_item
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="techem/components/home-stage-item" />
            </home-stage>
        </root>
        <experiencefragment-header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="techem/components/experiencefragment"
                fragmentVariationPath="/content/experience-fragments/techem/de/de/header/master2"
                id="landing-header"/>
        <experiencefragment-footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="techem/components/experiencefragment"
                fragmentVariationPath="/content/experience-fragments/techem/de/de/footer/master1"
                id="landing-footer"/>
    </jcr:content>
</jcr:root>